{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "arrow-body-style": "off",
        "func-names": "off",
        "class-methods-use-this": "off",
        "no-undef": "off",
        "no-param-reassign": "off",
        "import/prefer-default-export": "off"
    }
}
